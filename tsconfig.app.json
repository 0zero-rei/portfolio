/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@env": ["environments/environment"],
      "@core/*": ["app/core/*"],
      "@shared/*": ["app/shared/*"],
      "@interfaces/*": ["app/shared/interfaces/*"],
      "@models/*": ["app/shared/models/*"],
      "@services/*": ["app/shared/services/*"],
      "@helpers/*": ["app/shared/helpers/*"],
      "@utils/*": ["app/shared/utils/*"],
      "@validators/*": ["app/shared/validators/*"],
      "@modules/*": ["app/modules/*"]
    },
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
